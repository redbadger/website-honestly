  "config": {
    "pre-git": {
      "pre-commit": [
        "make scan-secrets",
        "make prettier-fail",
        "make lint",
        "make test"
      ]
    }
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.5.0",
    "cypress": "^4.1.0",
    "cypress-image-snapshot": "^3.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint-plugin-cypress": "^2.10.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "prettier": "^1.16.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "services/**/*.{js,jsx}",
      "site/**/*.{js,jsx}",
      "state/**/*.{js,jsx}",
      "!**/*.stories.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy",
      "^(.+)\\.(jpe?g|png)(\\?.+)$": "$1.$2"
    },
    "setupFiles": [
      "jest-canvas-mock",
      "<rootDir>/node_modules/regenerator-runtime/runtime"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/dev/jest/enzyme-config.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testURL": "https://red-badger.com",
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ejs|pug)$": "<rootDir>/dev/jest/file-transform.js"
    }
  }
