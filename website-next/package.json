{
  "name": "website-next",
  "version": "1.0.0",
  "private": true,
  "description": "Red Badger's website",
  "main": "build/server",
  "scripts": {
    "build": "npm run clean && webpack --progress",
    "populate": "source .env && node scripts/import-events-to-couchdb.js",
    "clean": "rimraf build",
    "lint": "eslint ./src",
    "local": "source .env && NODE_ENV=development HOT=true node server.dev.js",
    "security:check": "./node_modules/.bin/nsp check",
    "start": "node build/server",
    "test": "npm run test:server && npm run security:check",
    "test-full": "npm run test:server && npm run security:check && npm run lint && npm run test:e2e",
    "test:e2e": "wdio spec/wdio.config.js",
    "test:server": "mocha --compilers js:babel-core/register --require spec/mocha-helper.js --recursive --reporter spec $(find src -name *spec.js)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redbadger/website-next.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/redbadger/website-next/issues"
  },
  "homepage": "https://github.com/redbadger/website-next",
  "dependencies": {
    "body-parser": "^1.15.1",
    "chalk": "^1.1.1",
    "classnames": "^2.2.3",
    "date-fns": "^1.3.0",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "express-prismic": "^1.1.1",
    "express-session": "^1.13.0",
    "html-entities": "^1.2.0",
    "html-to-react": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.2.0",
    "lodash-addons": "^0.3.1",
    "marked": "^0.3.5",
    "moment": "^2.13.0",
    "nano": "^6.2.0",
    "newrelic": "^1.28.0",
    "param-case": "^1.1.2",
    "query-string": "^4.2.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-ga": "^2.1.1",
    "react-helmet": "^3.1.0",
    "react-redux": "4.2.1",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^3.0.0",
    "redux": "3.2.1",
    "redux-devtools": "3.0.2",
    "redux-thunk": "^1.0.3",
    "request": "^2.72.0",
    "serialize-javascript": "^1.1.2",
    "slug": "^0.9.1",
    "source-map-support": "^0.4.0"
  },
