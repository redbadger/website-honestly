    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "wdio": "^0.3.1",
    "webdriverio": "^3.4.0",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  },
  "engines": {
    "node": "5.5.0"
  },
  "config": {
    "pre-git": {
      "pre-commit": [
        "npm t",
        "npm run lint || { exit 1; }"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-checkout": [],
      "post-merge": []
    }
  }
}
