{
  "presets": [
    "es2015",
    "react"
  ],
  "plugins": [
    "transform-object-assign",
    "transform-class-properties",
    "transform-flow-strip-types",
    "babel-plugin-transform-object-rest-spread"
  ],
  "env": {
    "node": {
      "plugins": [
        ["transform-runtime", {
          "polyfill": true,
          "regenerator": true
        }],
        "transform-async-to-generator"
      ]
    }
  }
}
